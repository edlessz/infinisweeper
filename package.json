{
  "name": "infinisweeper",
  "private": true,
  "version": "3.0.1",
  "type": "module",
  "scripts": {
    "build": "pnpm tsc -b && pnpm vite build && pnpm exec cap sync",
    "dev": "pnpx vite",
    "dev:android": "pnpm exec cap sync android && pnpm exec cap run android",
    "preview": "pnpm vite preview",
    "write-cname": "node -e \"require('fs').writeFileSync('dist/CNAME', 'infinisweeper.net')\"",
    "deploy": "pnpm build && pnpm write-cname && pnpm gh-pages -d dist"
  },
  "dependencies": {
    "@capacitor/android": "^7.3.0",
    "@capacitor/app": "^7.0.1",
    "@capacitor/core": "^7.3.0",
    "@capacitor/ios": "^7.3.0",
    "@supabase/supabase-js": "^2.49.8",
    "fast-simplex-noise": "^4.0.0",
    "lucide-react": "^0.510.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "seedrandom": "^3.0.5"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.3.0",
    "@eslint/js": "^9.28.0",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@types/seedrandom": "^3.0.8",
    "@vitejs/plugin-react": "^4.5.0",
    "eslint": "^9.28.0",
    "eslint-plugin-react-dom": "^1.50.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-react-x": "^1.50.0",
    "gh-pages": "^6.3.0",
    "globals": "^15.15.0",
    "prettier": "^3.5.3",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.33.0",
    "vite": "^6.3.5"
  }
}
